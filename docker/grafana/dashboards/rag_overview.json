{
  "title": "RAG Overview",
  "uid": "rag-overview",
  "timezone": "browser",
  "panels": [
    {
      "type": "stat",
      "title": "Cache hit rate",
      "targets": [
        { "expr": "rag_cache_hits_total / (rag_cache_hits_total + rag_cache_misses_total)" }
      ],
      "gridPos": { "x": 0, "y": 0, "w": 8, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Request latency p95",
      "targets": [
        { "expr": "histogram_quantile(0.95, sum(rate(rag_request_latency_seconds_bucket[5m])) by (le))" }
      ],
      "gridPos": { "x": 8, "y": 0, "w": 16, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Tokens per request",
      "targets": [
        { "expr": "rate(rag_tokens_total[5m])" }
      ],
      "gridPos": { "x": 0, "y": 6, "w": 12, "h": 6 }
    },
    {
      "type": "timeseries",
      "title": "Errors rate",
      "targets": [
        { "expr": "rate(rag_errors_total[5m])" }
      ],
      "gridPos": { "x": 12, "y": 6, "w": 12, "h": 6 }
    }
  ],
  "schemaVersion": 37,
  "version": 1
}